{
  "parserOptions": {
    "ecmaVersion": "latest",
    "parser": "@babel/eslint-parser",
    "sourceType": "module"
  },
  "extends": [
    "airbnb",
    "plugin:prettier/recommended",
    "plugin:react/recommended",
    "plugin:react/jsx-runtime"
  ],
  "env": {
    "browser": true,
    "es6": true
  },
  "rules": { "react/function-component-definition": "off" }
}
